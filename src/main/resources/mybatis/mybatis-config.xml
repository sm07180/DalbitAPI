<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="aggressiveLazyLoading" value="false"/>
		<!--<setting name="mapUnderscoreToCamelCase" value="true"/>-->
	</settings>

	<typeAliases>
		<typeAlias type="com.dalbit.sample.vo.SampleVo" alias="SampleVo" />
		<typeAlias type="com.dalbit.common.vo.ProcedureVo" alias="ProcedureVo" />
		<typeAlias type="com.dalbit.common.vo.ProcedureOutputVo" alias="ProcedureOutputVo" />
		<typeAlias type="com.dalbit.broadcast.vo.P_RoomListVo" alias="P_RoomListVo" />
		<typeAlias type="com.dalbit.broadcast.vo.P_RoomMemberListVo" alias="P_RoomMemberListVo" />
		<typeAlias type="com.dalbit.member.vo.P_FanboardListVo" alias="P_FanboardListVo" />
	</typeAliases>

</configuration>
